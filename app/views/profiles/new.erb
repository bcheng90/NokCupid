<div class="row">
<section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Add Profile Information</h2>
                    <p class="section-subheading text-muted">Tell Us About Yourself</p>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                     <form action="/users/<%= new_user %>/profile" method="post">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input type="integer" class="form-control" name="age" placeholder="Your Age" id="age" >
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control" name="location" placeholder="Your Location *" id="location">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                      <input class="form-control" type='text' name="about_me" placeholder='About You' name='about_me'/>
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                              <div class="col-md-6">
                                <div class="form-group">
                                      <input class="form-control" type='text' name="quirk" placeholder='Your Quirk' name='quirk' />
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>

                             <div class="col-md-6">
                                <div class="form-group">
                                      <input class="form-control" type='text' name="tagline" placeholder='Your Tagline' name='tagline' />
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-xl">Add Profile</button>
                            </div>
                        </div>
                    </form>


                </div>
            </div>
        </div>
    </section>
</div>

