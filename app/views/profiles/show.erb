    <!-- About Section -->
    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Profile</h2>
                    <h3 class="section-subheading text-muted">User So And So</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <ul class="timeline">
                        <li>
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/1.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Age</h4>
                                    <h4 class="subheading"><%= profile.age %></h4>
                                </div>

                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/2.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Tagline</h4>
                                    <h4 class="subheading"><%= profile.tagline %></h4>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/3.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Quirrrrrkkk</h4>
                                    <h4 class="subheading"><%= profile.quirk %></h4>
                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="img/about/4.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Location</h4>
                                    <h4 class="subheading"><%= profile.location %></h4>
                                </div>
                            </div>
                        </li>
                    </ul>
                    <ul>Likes
                      <% profile.tags.each do |tag|%>
                        <% if tag.status == true %>
                            <li><%= tag.name.capitalize %> </li>
                        <% end %>
                      <% end %>
                  </ul>
                  <ul>Dislikes
                    <% profile.tags.each do |tag|%>
                      <% if tag.status == false %>
                          <li><%= tag.name.capitalize %> </li>
                      <% end %>
                    <% end %>
                  </ul>
                  <button class="btn"><a href="/profiles/<%=profile.id%>/edit">Edit Profile</a></button>
                </div>
            </div>
        </div>
    </section>
