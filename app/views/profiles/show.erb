    <!-- About Section -->
    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading"><%=user.username%></h2>
                    <h3 class="section-subheading text-muted"><%=profile.tagline%></h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <ul class="timeline">
                        <li>
                            <div class="timeline-image">
                                <img class="img-circle img-responsive" src="http://a-z-animals.com/media/animals/images/original/sea_lion10.jpg" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>Age</h4>
                                    <h5 class="subheading"><%= profile.age %></h5>
                                </div>
                                <div class="timeline-heading">
                                    <h4>Quirrrrrkkk</h4>
                                    <h5 class="subheading"><%= profile.quirk %></h5>
                                </div>
                                <div class="timeline-heading">
                                    <h4>Location</h4>
                                    <h5 class="subheading"><%= profile.location %></h5>
                                </div>

                            </div>
                    </ul>
                <div class='row'>
                <div class='col-md-6 right-justify'>
                  <ul><h4>Likes</h4>
                    <% profile.tags.each do |tag|%>
                      <% if tag.status == true %>
                          <li><%= tag.name.capitalize %> </li>
                      <% end %>
                    <% end %>
                  </ul>
                </div>

                <div class='col-md-6'>
                  <ul><h4>Dislikes</h4>
                    <% profile.tags.each do |tag|%>
                      <% if tag.status == false %>
                          <li><%= tag.name.capitalize %> </li>
                      <% end %>
                    <% end %>
                  </ul>
                </div>
              </div>
              <BR><BR><BR>

                  <div class='col-md-12 text-center' id='prof_edit_button'>
                    <button class="btn"><a href="/profiles/<%=profile.id%>/edit">Edit Profile</a></button>
                  </div>
                </div>
            </div>
        </div>
    </section>
